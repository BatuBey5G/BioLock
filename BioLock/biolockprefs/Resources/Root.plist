<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- Header -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>BioLock Settings</string>
			<key>footerText</key>
			<string>Protect your apps with Face ID or Touch ID. Changes take effect immediately.</string>
		</dict>
		
		<!-- Enable Switch -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.batues.biolock</string>
			<key>key</key>
			<string>Enabled</string>
			<key>label</key>
			<string>Enable BioLock</string>
			<key>PostNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
		</dict>
		
		<!-- Protected Apps -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Protected Apps</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSLinkCell</string>
			<key>detail</key>
			<string>ATLApplicationListMultiSelectionController</string>
			<key>defaults</key>
			<string>com.batues.biolock</string>
			<key>key</key>
			<string>ProtectedApps</string>
			<key>label</key>
			<string>Select Apps to Lock</string>
			<key>ALSettingsPath</key>
			<string>/var/mobile/Library/Preferences/com.batues.biolock.plist</string>
			<key>ALSettingsKeyPrefix</key>
			<string></string>
			<key>ALSettingsDefaultValue</key>
			<false/>
			<key>ALChangeNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
			<key>PostNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
		</dict>
		
		<!-- Authentication Settings -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Authentication Options</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.batues.biolock</string>
			<key>key</key>
			<string>AllowPasscode</string>
			<key>label</key>
			<string>Allow Passcode Fallback</string>
			<key>PostNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSLinkListCell</string>
			<key>default</key>
			<integer>0</integer>
			<key>defaults</key>
			<string>com.batues.biolock</string>
			<key>detail</key>
			<string>PSListItemsController</string>
			<key>key</key>
			<string>AuthCacheDuration</string>
			<key>label</key>
			<string>Remember Authentication</string>
			<key>validTitles</key>
			<array>
				<string>Disabled</string>
				<string>30 seconds</string>
				<string>1 minute</string>
				<string>5 minutes</string>
				<string>15 minutes</string>
				<string>30 minutes</string>
				<string>1 hour</string>
			</array>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>30</integer>
				<integer>60</integer>
				<integer>300</integer>
				<integer>900</integer>
				<integer>1800</integer>
				<integer>3600</integer>
			</array>
			<key>PostNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
		</dict>
		
		<!-- Customization -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Customization</string>
			<key>footerText</key>
			<string>Use %app% to insert the app name in your custom prompt.</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSEditTextCell</string>
			<key>defaults</key>
			<string>com.batues.biolock</string>
			<key>key</key>
			<string>CustomPrompt</string>
			<key>label</key>
			<string>Custom Prompt</string>
			<key>placeholder</key>
			<string>Unlock %app%</string>
			<key>PostNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
		</dict>
		
		<!-- Feedback Options -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Feedback</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.batues.biolock</string>
			<key>key</key>
			<string>VibrateOnFail</string>
			<key>label</key>
			<string>Vibrate on Failed Auth</string>
			<key>PostNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.batues.biolock</string>
			<key>key</key>
			<string>ShowNotification</string>
			<key>label</key>
			<string>Show Status Notifications</string>
			<key>PostNotification</key>
			<string>com.batues.biolock/ReloadPrefs</string>
		</dict>
		
		<!-- About -->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>About</string>
			<key>footerText</key>
			<string>BioLock Enhanced v1.1.0
Free, full-featured app locker for iOS 15+

Â© 2024 Batu</string>
		</dict>
		
		<dict>
			<key>action</key>
			<string>clearAuthCache:</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Clear Authentication Cache</string>
		</dict>
		
		<dict>
			<key>action</key>
			<string>resetSettings:</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>Reset All Settings</string>
			<key>isDestructive</key>
			<true/>
		</dict>
		
		<dict>
			<key>action</key>
			<string>openGitHub:</string>
			<key>cell</key>
			<string>PSButtonCell</string>
			<key>label</key>
			<string>GitHub Repository</string>
		</dict>
	</array>
	<key>title</key>
	<string>BioLock</string>
</dict>
</plist>
